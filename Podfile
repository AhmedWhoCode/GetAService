#Storage service
pod 'Firebase/Storage'
#google sign in
pod 'GoogleSignIn'
#drop down list
pod 'iOSDropDown'
#image picker
pod 'YPImagePicker'
#caching image
 pod 'Kingfisher', '~> 6.1'

  target 'GetAServiceTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GetAServiceUITests' do
    # Pods for testing
  end

end
